<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
                           http://www.springframework.org/schema/beans/spring-beans.xsd">



    <bean class="org.example.domain.Tesla">
        <qualifier type="org.example.annotation.ElonMuskCompany"/>
        <qualifier type="org.example.annotation.CarCompany"/>
    </bean>

    <bean class="org.example.domain.SpaceX">
        <qualifier type="org.example.annotation.ElonMuskCompany"/>
        <qualifier type="org.example.annotation.RocketCompany"/>
    </bean>
</beans>